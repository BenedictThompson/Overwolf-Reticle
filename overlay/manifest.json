{
  "manifest_version": "1",
  "type": "WebApp",
  "meta":{
    "name": "Reticle Overlay",
    "version": "15.2.10",
    "minimum-overwolf-version": "0.83.46.0",
    "author": "Jacob McIntosh (nacitar sevaht)",
    "description": "A simple overlay that adds a customizable crosshair to the center of your game window.  Supports transparency, saving and loading settings profiles, sharing your settings with friends, and using a URL to an external crosshair image for more elaborate usages.  A hotkey is used to open the settings window, and by default it is set to CTRL-SHIFT-Backspace but can be changed in the overwolf settings.",
    "icon": "IconMouseOver.png",
    "icon_gray": "IconMouseNormal.png",
    "store_icon": "store/Icon.png",
    "access-name": "com.ubercpp.reticle"
  },
  "data": {
    "windows": {
      "ReticleWindow": {
        "file": "files/reticle.html",
        "transparent": true,
        "clickthrough": true,
        "in_game_only": true,
        "size": {
          "width": 1024,
          "height": 768
        },
        "start_position": {
          "top": 0,
          "left": 0
        }
      },
      "SettingsWindow": {
        "file": "files/settings.html",
        "transparent": true,
        "in_game_only": true,
        "size": {
          "width": 1024,
          "height": 768
        },
        "start_position": {
          "top": 0,
          "left": 0
        }
      }
    },
    "start_window": "ReticleWindow",
    "externally_connectable": {
      "matches": []
    },
    "plugins": [],
    "hotkeys": {
      "reticleSettings": {
        "title": "Open reticle settings",
        "default": "Ctrl+Shift+Back",
        "action-type": "custom"
      },
      "quickSlot1": {
        "title": "Activate quick slot 1",
        "default": "Ctrl+Shift+1",
        "action-type": "custom"
      },
      "quickSlot2": {
        "title": "Activate quick slot 2",
        "default": "Ctrl+Shift+2",
        "action-type": "custom"
      },
      "quickSlot3": {
        "title": "Activate quick slot 3",
        "default": "Ctrl+Shift+3",
        "action-type": "custom"
      },
      "quickSlot4": {
        "title": "Activate quick slot 4",
        "default": "Ctrl+Shift+4",
        "action-type": "custom"
      },
      "quickSlot5": {
        "title": "Activate quick slot 5",
        "default": "Ctrl+Shift+5",
        "action-type": "custom"
      },
      "quickSlot6": {
        "title": "Activate quick slot 6",
        "default": "Ctrl+Shift+6",
        "action-type": "custom"
      },
      "quickSlot7": {
        "title": "Activate quick slot 7",
        "default": "Ctrl+Shift+7",
        "action-type": "custom"
      },
      "quickSlot8": {
        "title": "Activate quick slot 8",
        "default": "Ctrl+Shift+8",
        "action-type": "custom"
      },
      "quickSlot9": {
        "title": "Activate quick slot 9",
        "default": "Ctrl+Shift+9",
        "action-type": "custom"
      },
      "quickSlot10": {
        "title": "Activate quick slot 10",
        "default": "Ctrl+Shift+0",
        "action-type": "custom"
      }
    },
    "launch_events": []
  }
}
