{
  "manifest_version": "1",
  "type": "WebApp",
  "meta":{
    "name": "Reticle Overlay",
    "version": "1.1.0",
    "minimum-overwolf-version": "0.77.4.0",
    "author": "nacitar sevaht",
    "icon": "files/images/IconMouseOver.png",
    "icon_gray": "files/images/IconMouseNormal.png",
    "description": "Reticle Overlay"
  },
  "data": {
    "start_window": "ReticleWindow",
    "windows": {
      "ReticleWindow": {
        "file": "files/reticle.html",
        "transparent": true,
        "resizable": false,
        "clickthrough": true,
        "show_in_taskbar": false,
        "in_game_only": true,
        "size": {
          "width": 640,
          "height": 480
        },
        "start_position": {
          "top": 0,
          "left": 0
        }
      },
      "SettingsWindow": {
        "file": "files/settings.html",
        "transparent": true,
        "resizable": true,
        "clickthrough": false,
        "show_in_taskbar": false,
        "in_game_only": true,
        "size": {
          "width": 1024,
          "height": 768
        },
        "start_position": {
          "top": 0,
          "left": 0
        }
      }
    },
    "hotkeys": {
      "reticleMenu": {
        "title": "Toggle Reticle Menu",
        "action-type": "custom",
        "default": "Ctrl+Shift+Back"
      }
    }
  }
}
